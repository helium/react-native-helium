{
  "name": "@helium/react-native-sdk",
  "version": "0.1.0",
  "description": "Helium React Native SDK",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "helium-react-native-sdk.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "preinstall": "rm -rf node_modules",
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "docs": "typedoc"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/helium/react-native-helium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helium/react-native-helium/issues"
  },
  "homepage": "https://github.com/helium/react-native-helium#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@helium/crypto-react-native": "^3.9.0",
    "@helium/currency": "^3.25.1",
    "@helium/http": "^3.43.0",
    "@helium/proto-ble": "^3.37.0",
    "@helium/transactions": "^3.38.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/base-64": "^0.1.3",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.64.14",
    "base-64": "^1.0.0",
    "buffer": "^6.0.3",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "events": "^3.3.0",
    "h3-js": "^3.7.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "protobufjs": "^6.11.2",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-ble-plx": "^2.0.2",
    "react-native-builder-bob": "^0.18.1",
    "react-native-get-random-values": "^1.7.0",
    "react-native-sodium": "^0.3.9",
    "readable-stream": "^3.6.0",
    "release-it": "^14.2.2",
    "stream": "^0.0.2",
    "typedoc": "^0.21.9",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@helium/crypto-react-native": "^3.9.0",
    "@helium/currency": "^3.25.1",
    "@helium/http": "^3.43.0",
    "@helium/proto-ble": "^3.37.0",
    "@helium/transactions": "^3.38.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "h3-js": "^3.7.2",
    "protobufjs": "^6.11.2",
    "react": "*",
    "react-native": "*",
    "react-native-ble-plx": "^2.0.2",
    "react-native-get-random-values": "^1.7.0",
    "react-native-sodium": "^0.3.9",
    "readable-stream": "^3.6.0",
    "stream": "^0.0.2"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/",
      "<rootDir>/docs/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false,
          "semi": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "docs/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
